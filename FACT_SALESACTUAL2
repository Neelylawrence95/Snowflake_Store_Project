create or replace TABLE IMT577_DB_LAW_NEELY.PUBLIC.FACT_SALESACTUAL2 (
	DIMPRODUCTID NUMBER(38,0),
	DIMSTOREID NUMBER(38,0),
	DIMRESELLERID NUMBER(38,0),
	DIMCUSTOMERID NUMBER(38,0),
	DIMCHANNELID NUMBER(38,0),
	DIMSALEDATEID NUMBER(9,0),
	DIMLOCATIONID NUMBER(38,0),
	SALESHEADERID NUMBER(38,0),
	SALESDETAILID NUMBER(38,0),
	SALEAMOUNT NUMBER(38,0),
	SALEQUANTITY NUMBER(38,0),
	SALEUNITPRICE NUMBER(38,0),
	SALEEXTENDEDCOST NUMBER(38,0),
	SALETOTALPROFIT NUMBER(38,0),
	constraint FK_DIMPRODUCTID foreign key (DIMPRODUCTID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_PRODUCT(DIMPRODUCTID),
	constraint FK_DIMSTOREID foreign key (DIMSTOREID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_STORE(DIMSTOREID),
	constraint FK_RESELLERID foreign key (DIMRESELLERID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_RESELLER(DIMRESELLERID),
	constraint FK_CUSTOMERID foreign key (DIMCUSTOMERID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_CUSTOMER(DIMCUSTOMERID),
	constraint FK_CHANNELID foreign key (DIMCHANNELID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_CHANNEL(DIMCHANNELID),
	constraint FK_DIMSALEDATEID foreign key (DIMSALEDATEID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_DATE(DATE_PKEY),
	constraint FK_DIMLOCATIONID foreign key (DIMLOCATIONID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_LOCATION(DIMLOCATIONID)
);
