create or replace TABLE IMT577_DB_LAW_NEELY.PUBLIC.FACT_SRCSALESTARGET (
	DIMSTOREID NUMBER(38,0),
	DIMRESELLERID NUMBER(38,0),
	DIMCHANNELID NUMBER(38,0),
	DIMTARGETDATEID NUMBER(9,0),
	SALESTARGETAMOUNT NUMBER(38,0),
	constraint FK_DIMSTOREID foreign key (DIMSTOREID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_STORE(DIMSTOREID),
	constraint FK_RESELLERID foreign key (DIMRESELLERID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_RESELLER(DIMRESELLERID),
	constraint FK_CHANNELID foreign key (DIMCHANNELID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_CHANNEL(DIMCHANNELID),
	constraint FK_DIMTARGETDATEID foreign key (DIMTARGETDATEID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_DATE(DATE_PKEY)
);
