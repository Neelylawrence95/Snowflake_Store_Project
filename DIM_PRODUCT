create or replace TABLE IMT577_DB_LAW_NEELY.PUBLIC.DIM_PRODUCT (
	DIMPRODUCTID NUMBER(38,0) NOT NULL autoincrement start 1 increment 1 noorder,
	PRODUCTID NUMBER(38,0),
	PRODUCTTYPEID NUMBER(38,0),
	PRODUCTCATEGORYID NUMBER(38,0),
	PRODUCTNAME VARCHAR(255),
	PRODUCTTYPE VARCHAR(255),
	PRODUCTCATEGORY VARCHAR(255),
	PRODUCTRETAILPRICE FLOAT,
	PRODUCTWHOLESALEPRICE FLOAT,
	PRODUCTCOST FLOAT,
	PRODUCTRETAILPROFIT FLOAT,
	PRODUCTWHOLESALEUNITPROFIT FLOAT,
	PRODUCTPROFITMARGINUNITPERCENT VARCHAR(255),
	constraint PK_DIMPRODUCTID primary key (DIMPRODUCTID)
);
