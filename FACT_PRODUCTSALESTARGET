create or replace TABLE IMT577_DB_LAW_NEELY.PUBLIC.FACT_PRODUCTSALESTARGET (
	DIMPRODUCTID NUMBER(38,0),
	DIMTARGETDATEID NUMBER(9,0),
	PRODUCTTARGETSALESQUANTITY NUMBER(38,0),
	constraint FK_DIMPRODUCTID foreign key (DIMPRODUCTID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_PRODUCT(DIMPRODUCTID),
	constraint FK_DIMTARGETDATEID foreign key (DIMTARGETDATEID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_DATE(DATE_PKEY)
);
