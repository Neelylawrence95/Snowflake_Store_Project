create or replace TABLE IMT577_DB_LAW_NEELY.PUBLIC.DIM_CUSTOMER (
	DIMCUSTOMERID NUMBER(38,0) NOT NULL autoincrement start 1 increment 1 noorder,
	DIMLOCATIONID NUMBER(38,0) autoincrement start 1 increment 1 noorder,
	CUSTOMERID VARCHAR(255),
	CUSTOMERFULLNAME VARCHAR(255),
	CUSTOMERFIRSTNAME VARCHAR(255),
	CUSTOMERLASTNAME VARCHAR(255),
	CUSTOMERGENDER VARCHAR(255),
	constraint PK_DIMCUSTOMERID primary key (DIMCUSTOMERID),
	constraint FK_DIMLOCATIONID foreign key (DIMLOCATIONID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_LOCATION(DIMLOCATIONID)
);
