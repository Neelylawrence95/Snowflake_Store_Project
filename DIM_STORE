create or replace TABLE IMT577_DB_LAW_NEELY.PUBLIC.DIM_STORE (
	DIMSTOREID NUMBER(38,0) NOT NULL autoincrement start 1 increment 1 noorder,
	DIMLOCATIONID NUMBER(38,0) autoincrement start 1 increment 1 noorder,
	SOURCESTOREID NUMBER(38,0),
	STORENUMBER NUMBER(38,0),
	STOREMANAGER VARCHAR(255),
	constraint PK_DIMSTOREID primary key (DIMSTOREID),
	constraint FK_DIMLOCATIONID foreign key (DIMLOCATIONID) references IMT577_DB_LAW_NEELY.PUBLIC.DIM_LOCATION(DIMLOCATIONID)
);
