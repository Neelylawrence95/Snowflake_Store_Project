create or replace TABLE IMT577_DB_LAW_NEELY.PUBLIC.DIM_LOCATION (
	DIMLOCATIONID NUMBER(38,0) NOT NULL autoincrement start 1 increment 1 noorder,
	ADDRESS VARCHAR(255),
	CITY VARCHAR(255),
	POSTALCODE VARCHAR(255),
	STATEPROVINCE VARCHAR(255),
	COUNTRY VARCHAR(255),
	constraint PK_DIMLOCATIONID primary key (DIMLOCATIONID)
);
